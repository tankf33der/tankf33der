{
    "*.l": {
        "autoclose": false,
        "autoident": true,
        "filetype": "picolisp",
        "matchbrace": true,
        "matchbraceleft": true,
        "tabsize": 3,
        "tabstospaces": true
    },
    "*.vp": {
        "commenttype": "// %s",
        "filetype": "verifpal"
    },
    "autoclose": false,
    "cursorline": false,
    "rmtrailingws": true,
    "ruler": false
}
