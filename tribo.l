(de tribo (N)
   (cache 
      '(NIL)
      N
      (case N
         ((0 1) 0)
         (2 1)
         (T 
            (+ 
               (tribo (- N 1))
               (tribo (- N 2))
               (tribo (- N 3)) ) ) ) ) )
(bench
   (println
      (tribo 1000) ) )
(bye)

